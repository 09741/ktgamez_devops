<%- include("header") -%>
<!-- page content -->
<div id="content-page" class="content-page">
    <div class="container-fluid">
        <div class="row">
            <div class="col-sm-12">
                <% if(typeof alert != 'undefiend'){ %>
                    <% alert.forEach(function(error){ %>
                <div class="alert alert-warning alert-dismissible fade show" role="alert">
                  
                  <strong><%= error.msg %></strong>
                    <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                      </button>
                  </div>
                  <% }) %>
                  <% } %>
                <div class="iq-card">
                    <div class="iq-card-header d-flex justify-content-between">
                        <div class="iq-header-title">
                            <h4 class="card-title">Signup Bonus</h4>
                            
                        </div>
                    </div>
                    <div class="iq-card-body">
                        <div class="row">
                            <div class="col-lg-12">
                                                                
                                <form method="POST" action="signupbonus">
                                    <input type="hidden" name="_token" value="WHd7gLJXmzPTqzGnKOq1xT4buVJgnpF6cYeTcZKo">                                    

                                    <div class="row">
                                        <div class="col-lg-7">
                                            <div class="row">
                                                <div class="col-12 form-group">
                                                <label>Signup Bonus Tokens</label>
                                                <input type="hidden" class="form-control" placeholder="Signup Bonus Tokens" name="key" value="<%= data[0].key %>">

                                                    <input type="text" class="form-control" placeholder="Signup Bonus Tokens" name="SignupBonus" value="<%= data[0].value %>">
                                                </div>

                                               

                                                
                                            </div>
                                        </div>
                                        
                                    </div>

                                    
                                    <div class="row">
                                        <div class="col-lg-7 form-group ">
                                            <button type="submit" class="btn btn-primary">Submit</button>

                                        </div>
                                    </div>
                                </form>

                                <form method="POST" action="referralbonus">
                                    <input type="hidden" name="_token" value="WHd7gLJXmzPTqzGnKOq1xT4buVJgnpF6cYeTcZKo">                                    

                                    <div class="row">
                                        <div class="col-lg-7">
                                            <div class="row">
                                                <div class="col-12 form-group">
                                                    <label>Referral Bonus Tokens</label>
                                                    <input type="text" class="form-control" placeholder="Referral Bonus Tokens" name="ReferralBonus" value="<%= data[1].value %>">
                                                </div>

                                               

                                                
                                            </div>
                                        </div>
                                        
                                    </div>

                                    
                                    <div class="row">
                                        <div class="col-lg-7 form-group ">
                                            <button type="submit" class="btn btn-primary">Submit</button>

                                        </div>
                                    </div>
                                </form>


                                <form method="POST" action="leaderboardduration">

                                    <div class="row">
                                        <div class="col-lg-7">
                                            <div class="row">
                                                <div class="col-12 form-group">
                                                    <label>LeaderBoard Duration</label>
                                                    <br>
                                                    <% var date = JSON.parse(data[2].value) %>
                                                    <label>From Date - <%= date.FromDate %></label>
                                                    <input type="datetime-local" class="form-control" placeholder="FromDate" name="FromDate" value="<%= date.FromDate %>">
                                                    <label>To Date - <%= date.ToDate %></label>
                                                    <input type="datetime-local" class="form-control" placeholder="ToDate" name="ToDate" value="<%= date.ToDate %>">
                                                </div>

                                               

                                                
                                            </div>
                                        </div>
                                        
                                    </div>

                                    
                                    <div class="row">
                                        <div class="col-lg-7 form-group ">
                                            <button type="submit" class="btn btn-primary">Submit</button>

                                        </div>
                                    </div>
                                </form>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<%- include("footer") -%>
